# Redis Environment Variables Example
# Скопируйте этот файл в .env и измените значения

# Пароль для Redis
REDIS_PASSWORD=Royal_Redis_1337

# Настройки памяти (в байтах)
REDIS_MAXMEMORY=536870912

# Настройки сети
REDIS_PORT=6379
REDIS_HOST=0.0.0.0

# Настройки логирования
REDIS_LOGLEVEL=notice

# Настройки персистентности
REDIS_SAVE_900=1
REDIS_SAVE_300=10
REDIS_SAVE_60=10000

# Настройки AOF
REDIS_APPENDONLY=yes
REDIS_APPENDFSYNC=everysec 